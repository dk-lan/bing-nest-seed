{
  "name": "nest-seed",
  "version": "1.0.0",
  "description": "a framework base on nestjs",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": " webpack --progress",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nohup node dist/build.js > my_nohup.log 2>&1 &",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@enonic/fnv-plus": "^1.3.0",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.7.4",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "amqplib": "^0.5.5",
    "bip39": "^3.0.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cls-hooked": "^4.2.2",
    "coininfo": "^5.1.0",
    "coinkey": "^3.0.0",
    "decimal.js": "^10.2.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-wallet": "^0.6.3",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "neo4j-driver": "^1.7.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.11",
    "typescript": "^3.0.1",
    "web3": "^1.0.0-beta.37",
    "web3-core": "^1.0.0-beta.37",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
